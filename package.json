{
  "name": "shopping-list",
  "version": "1.0.0",
  "description": "MERN stack tutorial by Traversy media",
  "main": "server.js",
  "author": "theo-js <dev.js.theo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "client-install": "yarn --cwd client install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "yarn --cwd client start",
